<view class="_div"><swiper indicator-dots="{{swiper.indicatorDots}}" autoplay="{{swiper.autoplay}}" interval="{{swiper.interval}}" circular="{{swiper.circular}}" duration="{{swiper.duration}}" loop indicator-color="{{swiper.indicatorColor}}" indicator-active-color="{{swiper.indicatorActivColor}}"><block wx:for="{{imgUrls}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-e_id="{{item.e_id}}" data-event-opts="{{[['tap',[['goOtherPage',['$0','$1'],[[['imgUrls','',index,'e_id']],[['imgUrls','',index,'z_id']]]]]]]}}" bindtap="__e"><image src="{{item.hall_cover}}" class="_img"></image><block wx:if="{{item.showTitle}}"><text class="swiper-title">{{item.hall_name}}</text></block></swiper-item></block></swiper></view>